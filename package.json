{
  "name": "react-bs-datatable",
  "version": "2.0.0-alpha.1",
  "description": "React Bootstrap Datatable (without jQuery) with sorting, filter, and pagination",
  "main": "lib/Table.js",
  "scripts": {
    "test": "jest && yarn eslint",
    "jest:watch": "jest --watch",
    "eslint": "eslint --fix ./src/.",
    "webpack": "webpack",
    "build": "yarn build:lib && git add -A",
    "build:lib": "cd src && babel . --out-dir ../lib --ignore *.test.js",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Imballinst/react-bs-datatable.git"
  },
  "keywords": [
    "react",
    "bootstrap",
    "react-bootstrap",
    "datatable"
  ],
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js)$",
    "setupFiles": [
      "./test/jestsetup.js"
    ]
  },
  "author": "Try Ajitiono",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Imballinst/react-bs-datatable/issues"
  },
  "homepage": "https://github.com/Imballinst/react-bs-datatable#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.15.1",
    "jest": "^24.1.0",
    "react-test-renderer": "^16.8.4",
    "sinon": "^7.2.7",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "@storybook/react": "^5.0.10",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-links": "^5.0.10",
    "@storybook/addons": "^5.0.10"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.4"
  }
}
